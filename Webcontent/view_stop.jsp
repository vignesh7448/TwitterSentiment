    <tbody>
    <%
    ArrayList<String> al = (ArrayList<String>) request.getAttribute("stop");
	for (String s : al) {
		String line[] = s.split("\t\t");
		if (line.length >= 3) {
			String name = line[0];
			String time = line[1];
			String tweet = line[2];
		%>
      <tr>
        	
       		<td><%=name%></td>
			<td><%=time%></td>
			<td><%=tweet%></td>
      </tr>
      <%
			}
			}
		%>
    </tbody>
  </table>
